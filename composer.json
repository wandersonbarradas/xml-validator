{
    "name": "wandersonbarradas/xml-validator",
    "description": "Um pacote Laravel para validar arquivos XML contra o schema procNFe_v4.00.xsd.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Wanderson Barradas",
            "email": "wandersonbarradas07@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-dom": "*",
        "ext-libxml": "*",
        "illuminate/support": "^10.0 || ^11.0 || ^12.0"
    },
    "autoload": {
        "psr-4": {
            "WandersonBarradas\\XmlValidator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WandersonBarradas\\XmlValidator\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {}
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
